  TRG_CLIENT_ID,
  MATCH_CLIENT_ID,
  SRC_CTN_KEY,
  TRG_CTN_KEY,
  MATCH_CTN_KEY,
  SRC_IBAN,
  TRG_IBAN,
  MATCH_IBAN,
  SRC_XTRNL_KEY,
  TRG_XTRNL_KEY,
  MATCH_XTRNL_KEY,
  SRC_PRODUCT_KEY,
  TRG_PRODUCT_KEY,
  MATCH_PRODUCT_KEY,
  SRC_CREATE_DT,
  TRG_CREATE_DT,
  MATCH_CREATE_DT,

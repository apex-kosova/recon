grant select, update on MFC_MACC_TMN_FND_V to OWN_RECON;
grant select, insert, update, delete on MFC_MACC_TMN_FND_V to USR_LOGGING;
grant select, insert, update on MFC_MACC_TMN_FND_V to USR_RECONCILIATION;


